ig, final String configPath,\n            final KnownConfigValue[] fallBackValues)", "testcase": false, "constructor": false, "invocations": ["validateArgument", "validateArgument", "validateArgument", "hasPath", "newInstance", "empty", "withFallback", "arrayToConfig", "getConfigPath"]}, "repository": {"repo_id": 87849739, "url": "https://github.com/eclipse/ditto", "stars": 139, "created": "4/10/2017 7:29:57 PM +00:00", "updates": "2020-01-27T14:02:39+00:00", "fork": "False", "license": "licensed"}}